{
  "name": "ai_autonomous_data_manager",
  "version": "1.0.0",
  "description": "Node.js TypeScript project",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "dotenv -- tsx src/index.ts",
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.node.json && vite build",
    "dev:back": "dotenv -- tsx watch src/index.ts",
    "dev:front": "vite",
    "dev": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "start:prod": "npm run build && dotenv -- node dist/index.js"
  },
